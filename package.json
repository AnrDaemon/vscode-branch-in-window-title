{
	"name": "branch-in-window-title",
	"displayName": "Branch in Window Title",
	"description": "Visual Studio Code extension that shows the current Git branch in the window title. This can be used to automate time allocation to issue tracking systems via WiseTime.",
	"publisher": "wisetime",
	"version": "0.1.2",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/wisetime-io/vscode-branch-in-window-title.git"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"git",
		"branch",
		"window",
		"title",
		"wisetime"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"title": "Branch in Window Title",
			"properties": {
				"branchInWindowTitle.branchPollingInterval": {
					"type": [
						"number",
						"null"
					],
					"default": 3000,
					"description": "Specifies the Git branch polling interval in milliseconds."
				}
			}
		}
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^12.11.7",
		"@types/ramda": "^0.26.42",
		"@types/text-encoding": "0.0.35",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
		"eslint": "^7.5.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"text-encoding": "^0.7.0",
		"typescript": "^3.9.7",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"ramda": "^0.27.0"
	}
}
